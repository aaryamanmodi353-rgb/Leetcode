class Solution{
public ListNode oddEvenList(ListNode head){
if(head == null || head.next == null){
    return head;
}

ListNode curr = head;
List <Integer> lst = new ArrayList<>();


while(curr != null){
lst.add(curr.val);
curr = curr.next.next;
}
curr = head.next;

while(curr != null){
lst.add(curr.val);
curr = curr.next.next;

}

curr = head;
int count = 0;

while(curr != null && count < lst.size()){
curr.val = list.get(count);
count++;
curr = curr.next;

}

return head;


    }


}


//Best approach

class Solution {
    public ListNode oddEvenList(ListNode head) {

        if(head == null || head.next == null){
            return head;
        }

        ListNode odd = head;
        ListNode even = head.next;
        ListNode evenReal = head.next;



        while(even != null && even.next != null){

            odd.next = odd.next.next;
            odd = odd.next;

            even.next = even.next.next;
            even = even.next;
        }

        
        odd.next = evenReal;


        return head;
        
    }
}











